% ImageClass function
% Resample image
function stat = resample(self,ix,iy,iz)
% Inputs:   ix = dim1 indices to keep (empty keeps all)
%           iy = dim2 
%           iz = dim3 

stat = false;
if self.check && (nargin==4)
    if isempty(ix)
        ix = 1:self.dims(1);
    end
    if isempty(iy)
        ix = 1:self.dims(1);
    end
    if isempty(ix)
        ix = 1:self.dims(1);
    end
end